{
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "sourceMap": false,
    "target": "es2015",
    "outDir": "./build"
  },
  "include": ["src/*.ts"],
  "exclude": ["node_modules"]
}
